 apt-get install -y iptables
 apt-get install -y openvpn mariadb-server netcat httpie php neofetch vnstat
 apt-get install -y screen squid stunnel4 dropbear gnutls-bin python
 apt-get install -y dos2unix nano unzip jq virt-what net-tools default-mysql-client
